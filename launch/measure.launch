<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="sensor" default="1"/>
    <arg name="calibrate" default="0"/>

	<node name="measure_ta11" pkg="tiago_tactile" type="measure.py" args="--sensor $(arg sensor) --calibrate $(arg calibrate)" output="screen">
		<rosparam command="load" file="$(find tiago_tactile)/config/ta11.yaml" />
	</node>
</launch>